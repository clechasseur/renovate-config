{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "helpers:pinGitHubActionDigests",
    "helpers:disableTypesNodeMajor",
    "npm:unpublishSafe",
    ":disableRateLimiting"
  ],
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "rangeStrategy": "bump"
    },
    {
      "matchManagers": ["github-actions"],
      "schedule": ["weekly"]
    }
  ]
}
